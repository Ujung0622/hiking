<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="after">
    <insert id="insertAfter" parameterType="map">
        <![CDATA[
            INSERT INTO groupAfter
            VALUES (
                   #{userNum},
                   #{afterNum},
                   #{groupNum},
                   #{mtNm},
                   #{content},
                   #{fileSize},
                   #{storedFileName},
                   sysdate,
                   sysdate
                   )
        ]]>
    </insert>
</mapper>